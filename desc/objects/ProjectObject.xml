<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
      
  <name>ProjectObject</name>
      
  <description/>
      
  <version>1.0</version>
      
  <vendor/>
    
  <types>
        
    <typeDescription>
            
      <name>objects.ProjectObject</name>
            
      <description/>
            
      <supertypeName>uima.tcas.Annotation</supertypeName>
            
      <features>
                
        <featureDescription>
                    
          <name>Name</name>
                    
          <description>The project name.</description>
                    
          <rangeTypeName>uima.cas.String</rangeTypeName>
                  
        </featureDescription>
                
        <featureDescription>
                    
          <name>ProjectType</name>
                    
          <description>The project type. Currently, the only supported type is dual_validation. </description>
                    
          <rangeTypeName>uima.cas.String</rangeTypeName>
                  
        </featureDescription>
                
        <featureDescription>
                    
          <name>CheckoutTimeout</name>
                    
          <description>The value of the checkout timeout. </description>
                    
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
                  
        </featureDescription>
                
        <featureDescription>
                    
          <name>ProjectOwner</name>
                    
          <description>This will specify the user name of the project owner. If this user does not exist (case senseative), it will be created and assigned as the owner. </description>
                    
          <rangeTypeName>uima.cas.String</rangeTypeName>
                  
        </featureDescription>
              
      </features>
          
    </typeDescription>
  </types>
  
</typeSystemDescription>
