<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>  <annotatorImplementationName>annotators.SentenceSplittingAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>SentenceSplittingAnnotator</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>SentenceModelFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    <configurationParameter>
        <name>TokenModelFile</name>
        <description>Token model file location.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>POSTagger</name>
        <description>POS tagger. A maximum entropy tagger is optimal.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>NameFinderModelFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
