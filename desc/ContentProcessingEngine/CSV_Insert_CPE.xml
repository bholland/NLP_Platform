        </value>
      </nameValuePair>
    <nameValuePair>
        <name>DatabaseType</name>
        <value>
          <string>pgsql</string>
        </value>
      </nameValuePair>
    <nameValuePair>
        <name>CsvFile</name>
        <value>
          <array>
            <string>/home/ben/workspace/comment_counts/comment_counts_html_files.csv</string>
            <string>/home/ben/workspace/comment_counts/mass_campaigns.csv</string>
          </array>
